{
  "name": "fiexpress",
  "version": "2.0.0",
  "description": "NestJS CLI-like tool for Express.js projects",
  "keywords": [
    "expressjs",
    "nodejs",
    "express",
    "cli",
    "generator",
    "scaffolding",
    "expressjs-cli"
  ],
  "homepage": "https://github.com/developersailor/fiexpress#readme",
  "bugs": {
    "url": "https://github.com/developersailor/fiexpress/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/developersailor/fiexpress.git"
  },
  "license": "MIT",
  "author": "Mehmet Fiskindal",
  "type": "module",
  "main": "bin/create-fiexpress.js",
  "bin": {
    "fiexpress": "bin/create-fiexpress.js"
  },
  "files": [
    "bin",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "echo 'Publishing FiExpress CLI...'",
    "lint": "eslint bin --ext .js",
    "lint:fix": "eslint bin --ext .js --fix"
  },
  "dependencies": {
    "degit": "^2.8.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "lint-staged": "^15.0.0",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "bin/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
